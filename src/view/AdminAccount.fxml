<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.controller.AdminAccountController">
   <children>
      <Label layoutX="54.0" layoutY="14.0" text="Account Panel">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TableView fx:id="AdminAccountTbl" layoutX="3.0" layoutY="78.0">
         <columns>
            <TableColumn fx:id="numberCol" text="Number" />
            <TableColumn fx:id="nameCol" text="Name" />
            <TableColumn fx:id="balanceCol" text="Balance" />
            <TableColumn fx:id="typeCol" text="Type" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</Pane>
