<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.controller.AdminAccountController">
   <children>
      <Label layoutX="17.0" layoutY="14.0" text="Account Panel">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TableView fx:id="AdminAccountTbl" layoutX="3.0" layoutY="78.0">
         <columns>
            <TableColumn fx:id="numberCol" text="Number" />
            <TableColumn fx:id="balanceCol" text="Balance" />
            <TableColumn fx:id="typeCol" text="Type" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</Pane>
